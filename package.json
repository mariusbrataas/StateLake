{
  "name": "statelake",
  "version": "1.1.0",
  "description": "",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "clean": "rm -rf build",
    "build": "tsc && tsc -p ./tsconfig.nocomments.json && npm run minify",
    "cleanbuild": "npm run clean && npm run build",
    "bump": "npm version patch",
    "prepublishOnly": "npm run cleanbuild && npm run bump",
    "minify": "cd build && echo 'Minifying' && for FILE in *.js; do echo ' -' $FILE && ../node_modules/minify/bin/minify.js $FILE > tmp_mini && cat tmp_mini > $FILE; done && rm tmp_mini && cd .."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "typescript": "^4.2.0"
  },
  "devDependencies": {
    "@types/react": ">=16.8",
    "minify": "^7.0.2",
    "prettier": "^2.2.1",
    "react": ">=16.8"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "files": [
    "build"
  ],
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "jsxBracketSameLine": false,
    "printWidth": 80,
    "proseWrap": "preserve",
    "requirePragma": false,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false,
    "vueIndentScriptAndStyle": true,
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "printWidth": 200
        }
      }
    ]
  }
}
